                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k+3].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k+3].descripcion %>
                            </div>
                          </div>
                        </div>

                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k+4].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k+4].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k+4].descripcion %>
                            </div>
                          </div>
                        </div>


                      </div>
                      <% k = k+4 %>
                  <% else %>
                      <!-- Items -->
                      <div class="item">
                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k].descripcion %>
                            </div>
                          </div>
                        </div>
                      </div>
                      <% k = k+4 %>
                  <% end %>
              <% end %>
          <% end %>

        </div>
        <a data-slide="prev" href="#CarouselProyectos" class="left carousel-control carousel-control-proyectos"><i class="material-icons">chevron_left</i></a>
        <a data-slide="next" href="#CarouselProyectos" class="right carousel-control carousel-control-proyectos"><i class="material-icons">chevron_right</i></a>

      </div>
    </div>





  </div>
</div>






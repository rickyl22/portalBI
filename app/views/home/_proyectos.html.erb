<a name="project"></a>
 <div class="espacio">

 </div>
<div class="android-screen-section grip-proyectos">
  <img claSS="fondo-proyectos" src="assets/ideas.jpg">

  <div class="android-more-section android-screen-section">
  <div class="android-section-title mdl-typography--display-1-color-contrast titulo-proyecto">Nuestros Proyectos</div>
    <% proyectos = Proyecto.all
       cant_slider2 = proyectos.length / 4
       if (proyectos.length % 4 != 0)
         cant_slider2 = cant_slider2 + 1
       end
       cant_proyectos = proyectos.length
       k = 0
       l = 0
    %>
    <div class="mdl-grid">
      <div id="CarouselProyectos" class="carousel slide">
        <ol class="carousel-indicators carousel-indicators-proyectos">
          <% while l<cant_slider2 %>
              <% if l == 0 %>
                  <li data-target="#CarouselProyectos" data-slide-to="<%= l%>" class="active"></li>
              <% else %>
                  <li data-target="#CarouselProyectos" data-slide-to="<%=l %>"></li>
              <% end %>
              <% l = l+1 %>
          <% end %>
        </ol>

        <div class="carousel-inner">
          <% while k < cant_proyectos %>
              <% if k == 0%>
                  <% if (k+4) < cant_proyectos %>
                      <div class="item active">
                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k].descripcion %>
                            </div>
                          </div>
                        </div>

                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k+1].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k+1].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k+1].descripcion %>
                            </div>
                          </div>
                        </div>

                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k+2].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k+2].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k+2].descripcion %>
                            </div>
                          </div>
                        </div>

                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k+3].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k+3].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k+3].descripcion %>
                            </div>
                          </div>
                        </div>

                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k+4].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k+4].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k+4].descripcion %>
                            </div>
                          </div>
                        </div>

                      </div>
                      <% k = k+4 %>
                  <% else %>
                      <!-- Items -->
                      <div class="item active">
                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k].descripcion %>
                            </div>
                          </div>
                        </div>
                      </div>
                      <% k = k+4 %>
                  <% end %>
              <% else %>
                  <% if (k+4) < cant_proyectos %>
                      <div class="item">
                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k].descripcion %>
                            </div>
                          </div>
                        </div>

                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k+1].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k+1].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">
                              <%= proyectos[k+1].descripcion %>
                            </div>
                          </div>
                        </div>

                        <div class="mdl-cell mdl-cell--3-col mdl-cell--4-col-tablet mdl-cell--4-col-phone mdl-card mdl-shadow--3dp">
                          <div class="mdl-card__media mdl-card__media-proyectos">
                            <%= image_tag(proyectos[k+2].imagen_url.to_s) %>
                          </div>
                          <div class="mdl-card__supporting-text text-proyecto">
                            <h3 class="mdl-card__title-text"><span><%= proyectos[k+2].titulo %></span></h3>
                            <br>
                            <div class="descp_proyecto">

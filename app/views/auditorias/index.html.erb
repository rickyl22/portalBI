<p id="notice"><%= notice %></p>

<h1>Auditorias</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auditorias.each do |auditoria| %>
      <tr>
        <td><%= link_to 'Show', auditoria %></td>
        <td><%= link_to 'Edit', edit_auditoria_path(auditoria) %></td>
        <td><%= link_to 'Destroy', auditoria, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auditoria', new_auditoria_path %>

<p id="notice"><%= notice %></p>

<h1>Noticia</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @noticia.each do |noticias| %>
      <tr>
        <td><%= link_to 'Show', noticias %></td>
        <td><%= link_to 'Edit', edit_noticias_path(noticias) %></td>
        <td><%= link_to 'Destroy', noticias, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Noticia', new_noticias_path %>
